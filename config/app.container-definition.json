{
  "name": "app-{{ .environment }}",
  "image": "{{ .image }}",
  "essential": true,
  "entryPoint": [
    "/bin/milmove",
    "serve",
    "--db-env",
    "container",
    "--logging-env",
    "production",
    "--debug-logging",
    "--log-task-metadata",
    "--tls-enabled"
  ],
  "command": [],
  "portMappings": [
    {
      "containerPort": 8443,
      "hostPort": 8443,
      "protocol": "tcp"
    }
  ],
  "command": [],
  "secrets": {{ .secrets }},
  "environment": [
    {
      "name": "DB_IAM",
      "value": "{{ .DB_IAM }}"
    },
    {
      "name": "ENVIRONMENT",
      "value": "{{ .environment }}"
    },
    {
      "name": "DB_HOST",
      "value": "{{ .db_host }}"
    },
    {
      "name": "DB_PORT",
      "value": "5432"
    },
    {
      "name": "DB_USER",
      "value": "{{ .DB_USER }}"
    },
    {
      "name": "DB_NAME",
      "value": "app"
    },
    {
      "name": "DB_SSL_MODE",
      "value": "{{ .DB_SSL_MODE }}"
    },
    {
      "name": "DB_SSL_ROOT_CERT",
      "value": "{{ .DB_SSL_ROOT_CERT }}"
    },
    {
      "name": "DEVLOCAL_AUTH",
      "value": "{{ .DEVLOCAL_AUTH }}"
    },
    {
      "name": "HTTP_MY_SERVER_NAME",
      "value": "my.{{ .domain }}"
    },
    {
      "name": "HTTP_OFFICE_SERVER_NAME",
      "value": "office.{{ .domain }}"
    },
    {
      "name": "HTTP_ADMIN_SERVER_NAME",
      "value": "admin.{{ .domain }}"
    },
    {
      "name": "AWS_CF_DOMAIN",
      "value": "assets.{{ .domain }}"
    },
    {
      "name": "AWS_S3_BUCKET_NAME",
      "value": "transcom-ppp-app-{{ .environment }}-us-west-2"
    },
    {
      "name": "AWS_S3_REGION",
      "value": "{{ .AWS_S3_REGION }}"
    },
    {
      "name": "AWS_S3_KEY_NAMESPACE",
      "value": "app"
    },
    {
      "name": "STORAGE_BACKEND",
      "value": "{{ .STORAGE_BACKEND }}"
    },
    {
      "name": "EMAIL_BACKEND",
      "value": "ses"
    },
    {
      "name": "HERE_MAPS_GEOCODE_ENDPOINT",
      "value": "https://geocoder.cit.api.here.com/6.2/geocode.json"
    },
    {
      "name": "HERE_MAPS_ROUTING_ENDPOINT",
      "value": "https://route.cit.api.here.com/routing/7.2/calculateroute.json"
    },
    {
      "name": "LOGIN_GOV_HOSTNAME",
      "value": "{{ .LOGIN_GOV_HOSTNAME }}"
    },
    {
      "name": "DOD_CA_PACKAGE",
      "value": "/config/tls/Certificates_PKCS7_v5.4_DoD.der.p7b"
    },
    {
      "name": "IWS_RBS_HOST",
      "value": "{{ .IWS_RBS_HOST }}"
    },
    {
      "name": "HTTP_SDDC_SERVER_NAME",
      "value": "{{ .HTTP_SDDC_SERVER_NAME }}"
    },
    {
      "name": "HTTP_SDDC_PROTOCOL",
      "value": "https"
    },
    {
      "name": "HTTP_SDDC_PORT",
      "value": "{{ .HTTP_SDDC_PORT }}"
    },
    {
      "name": "GEX_URL",
      "value": "https://gexweba.daas.dla.mil/msg_data/submit/"
    },
    {
      "name": "GEX_SEND_PROD_INVOICE",
      "value": "{{ .GEX_SEND_PROD_INVOICE }}"
    },
    {
      "name": "SERVE_SWAGGER_UI",
      "value": "{{ .SERVE_SWAGGER_UI }}"
    },
    {
      "name": "FEATURE_FLAG_ACCESS_CODE",
      "value": "{{ .FEATURE_FLAG_ACCESS_CODE }}"
    },
    {
      "name": "DEBUG_PPROF",
      "value": "{{ .DEBUG_PPROF }}"
    },
    {
      "name": "SERVE_ADMIN",
      "value": "{{ .SERVE_ADMIN }}"
    },
    {
      "name": "SERVE_SDDC",
      "value": "{{ .SERVE_SDDC }}"
    },
    {
      "name": "SERVE_API_INTERNAL",
      "value": "{{ .SERVE_API_INTERNAL }}"
    },
    {
      "name": "SERVE_API_GHC",
      "value": "{{ .SERVE_API_GHC }}"
    }
  ],
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "ecs-tasks-app-{{ .environment }}",
      "awslogs-region": "us-west-2",
      "awslogs-stream-prefix": "app"
    }
  },
  "mountPoints": [],
  "volumesFrom": [],
  "readonlyRootFilesystem": true
}
