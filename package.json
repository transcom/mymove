    "resetMocks": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{jsx,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 35,
        "lines": 40,
        "statements": 40
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(imask)/)/"
